{
  "name": "@ovendjs/muzzle",
  "version": "1.1.0",
  "description": "A highly customizable text filtering system with extensive banned word source options",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "prepublishOnly": "npm run build",
    "benchmark": "node benchmarks/text-filtering.js"
  },
  "keywords": [
    "content-filtering",
    "moderation",
    "text-filtering",
    "profanity-filter",
    "content-moderation",
    "word-filtering",
    "banned-words"
  ],
  "author": "Oven",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.0",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "eslint": "^8.37.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.5.0",
    "prettier": "^2.8.7",
    "rimraf": "^4.4.0",
    "ts-jest": "^29.0.5",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "eventemitter3": "^4.0.7"
  },
  "optionalDependencies": {
    "node-fetch": "^2.6.7"
  },
  "peerDependencies": {
    "node-fetch": "^2.6.7"
  },
  "peerDependenciesMeta": {
    "node-fetch": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ovendjs/muzzle.git"
  },
  "bugs": {
    "url": "https://github.com/ovendjs/muzzle/issues"
  },
  "homepage": "https://github.com/ovendjs/muzzle#readme"
}